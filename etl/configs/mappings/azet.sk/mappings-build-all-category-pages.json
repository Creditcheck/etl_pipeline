{
  "record_selector": "div.subCategories span.category",
  "mappings": [
    {
      "selector": "span.category a",
      "extract": "attr",
      "attr": "href",
      "json_path": "href"
    },
    {
      "selector": "span.recordCount",
	  "match": ".*?([0-9]\\s?[0-9]+).*",
      "extract": "text",
      "json_path": "count"
    }
  ]
}

